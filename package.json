{
    "name": "warehouse-ms-be",
    "version": "0.0.1",
    "description": "Warehouse Management System",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "build:swc": "nest build -b swc --type-check",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:swc": "nest start -b swc --type-check",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --detectOpenHandles --forceExit --runInBand",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:generate ./migrations/$npm_config_name -d ./migrations/configs/db.config.ts",
        "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./migrations/$npm_config_name",
        "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run -d ./migrations/configs/db.config.ts",
        "db:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli schema:sync -d ./migrations/configs/db.config.ts",
        "db:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs db:create -d ./migrations/configs/db.config.ts",
        "db:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs db:drop -d ./migrations/configs/db.config.ts",
        "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./migrations/configs/db.config.ts",
        "seed:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:create -d ./migrations/configs/db.config.ts",
        "prisma:pull": "npx prisma db pull",
        "prisma:generate": "npx prisma generate",
        "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s",
        "generate:module": "node ./scripts/generate-module.js",
        "generate:entity": "node ./scripts/generate-entity.js",
        "generate:repository": "node ./scripts/generate-repository.js"
    },
    "dependencies": {
        "@golevelup/nestjs-discovery": "^4.0.0",
        "@nestjs-modules/mailer": "^1.9.1",
        "@nestjs/cli": "^10.2.1",
        "@nestjs/common": "^10.3.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.3.0",
        "@nestjs/event-emitter": "^2.0.3",
        "@nestjs/platform-express": "^10.3.0",
        "@nestjs/platform-socket.io": "^10.3.0",
        "@nestjs/schedule": "^3.0.4",
        "@nestjs/swagger": "^7.1.17",
        "@nestjs/typeorm": "10.0.0",
        "@nestjs/websockets": "^10.3.0",
        "@prisma/client": "^5.7.1",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cron": "^2.4.4",
        "excel4node": "^1.8.2",
        "exceljs": "^4.4.0",
        "faker": "^6.6.6",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        "mysql2": "^3.6.5",
        "nestjs-i18n": "^10.4.0",
        "nestjs-pino": "^3.5.0",
        "nestjs-redis": "github:skunight/nestjs-redis",
        "pino-pretty": "^10.3.1",
        "read-excel-file": "^5.7.1",
        "reflect-metadata": "^0.1.14",
        "rimraf": "^5.0.5",
        "rxjs": "^7.8.1",
        "socket.io": "^4.7.1",
        "swagger-ui-express": "^5.0.0",
        "typeorm": "0.3.17",
        "typeorm-extension": "^3.2.0",
        "typeorm-naming-strategies": "^4.1.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.23",
        "@nestjs/schematics": "^10.0.3",
        "@nestjs/testing": "^10.3.0",
        "@swc/cli": "^0.1.63",
        "@swc/core": "^1.3.101",
        "@types/express": "^4.17.21",
        "@types/faker": "^6.6.9",
        "@types/ioredis": "^5.0.0",
        "@types/jest": "^29.5.11",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.5",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "7.2.0",
        "eslint-plugin-prettier": "^3.4.1",
        "jest": "^29.7.0",
        "prettier": "^2.8.8",
        "prisma": "^5.7.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "./",
        "modulePaths": [
            "./"
        ],
        "moduleNameMapper": {
            "~(.*)$": "<rootDir>/src/$1"
        },
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "testRegex": ".*\\.(test|spec)\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "prisma": {
        "schema": "src/database/prisma/schema/schema.prisma"
    }
}
